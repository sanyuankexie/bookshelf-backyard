<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://sql.visualdust.com:3306/kxbookshelf?serverTimezone=GMT%2b8&amp;characterEncoding=UTF-8"
                        userId="root"
                        password="bassword">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="org.kexie.bookshelfbackyard.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="org.kexie.bookshelfbackyard.mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="books" domainObjectName="Book">
            <generatedKey column="guid" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="members" domainObjectName="Member">
            <generatedKey column="stu_id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="users" domainObjectName="User">
            <generatedKey column="uid" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="bookingrec" domainObjectName="BookingRecord">
            <generatedKey column="rec_id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="wechatUser" domainObjectName="UserWechat">
            <generatedKey column="uid" sqlStatement="MySql" identity="true"/>
        </table>

    </context>
</generatorConfiguration>